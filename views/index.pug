include head
body
  .wrap
    .content
      include header/header
      .container
        .row
          each val2, index2 in category
            .col.col-lg-12
              h2
                a(href=`/category?id=${val2['id']}`) #{val2['category']}

            each val, index in goods
              if val2['id'] == val['category']
                include card/card
    include navigation/navigation
    include footer/footer